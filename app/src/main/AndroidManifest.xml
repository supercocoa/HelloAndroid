<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.helloandroid.app">


    <uses-permission android:name="android.permission.INTERNET"/>

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:name="android.support.multidex.MultiDexApplication">
        <activity
                android:name=".MainActivity"
                android:label="@string/app_name"
                android:theme="@style/AppTheme">

            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>

        </activity>

        <activity
                android:name="com.helloandroid.image.SvgAndroidActivity"
                android:theme="@style/AppTheme">
        </activity>

        <activity
                android:name="com.helloandroid.image.VectorCompatActivity"
                android:theme="@style/AppTheme">
        </activity>

        <activity
                android:name="com.helloandroid.image.FrescoActivity"
                android:theme="@style/AppTheme">
        </activity>

        <activity
                android:name="com.helloandroid.image.PicassoActivity"
                android:theme="@style/AppTheme">
        </activity>

        <activity
                android:name="com.helloandroid.framework.RxAndroidActivity"
                android:theme="@style/AppTheme">
        </activity>

        <activity
                android:name="com.helloandroid.net.OkHttpActivity"
                android:theme="@style/AppTheme">
        </activity>

    </application>


</manifest>
